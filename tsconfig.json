{
    "compilerOptions": {
        "target": "es2015",
        "moduleResolution": "node",
        "strict": false,
        "declaration": true,
        "noUnusedLocals": true,
        "esModuleInterop": true,
        "outDir": "dist",
        "module": "commonjs",
        "lib": ["ESNext", "DOM"],
        "sourceMap": false,
    },
    "include": [
        "./src",
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}